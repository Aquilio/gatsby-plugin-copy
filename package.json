{
  "name": "gatsby-plugin-copy",
  "version": "0.0.1",
  "description": "Gatsby plugin to copy files and directories",
  "author": "Curtis Cummings <curtis@aquil.io>",
  "license": "MIT",
  "respository": {
    "type": "git",
    "url": "https://github.com/aquilio/gatsby-plugin-copy"
  },
  "scripts": {
    "build": "babel src --out-dir .",
    "watch": "babel -w src --out-dir .",
    "prepublishOnly": "cross-env NODE_ENV=production yarn run build",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "keywords": [
    "gatsby",
    "gatsby-plugin"
  ],
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "jest": "^21.0.2",
    "yarn": "^1.3.2"
  },
  "dependencies": {
    "cross-env": "^5.0.5",
    "fs-extra": "^4.0.2"
  }
}
